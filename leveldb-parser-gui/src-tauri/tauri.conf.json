{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "leveldb-parser-gui",
  "version": "1.0.0",
  "identifier": "dev.huebi.leveldb-parser-gui",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "LevelDB Parser",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/16x16.png",
      "icons/24x24.png",
      "icons/32x32.png",
      "icons/48x48.png",
      "icons/64x64.png",
      "icons/256x256.png",
      "icons/icon.ico"
    ]
  }
}